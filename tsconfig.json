{
    "compilerOptions": {
      "target": "ES2022",
      "module": "ES2022",
      "rootDir": "./src",
      "moduleResolution": "Node",
      "declaration": true,
      "outDir": "./build",
      "strict": true,
      "noImplicitAny": true,
      "suppressImplicitAnyIndexErrors": false,
      "skipLibCheck": true
    }
}